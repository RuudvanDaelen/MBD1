<?xml version='1.0' encoding='utf-8'?>
<widget id="nl_avans_mbd1_2060492_rdaelen" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pokedex</name>
    <description>Mobile Development 1 - Pokedex</description>
    <author email="rvdaelen@hotmail.com">Ruud van Daelen</author>
    <content src="index.html" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <icon src="icon.png" />
    <gap:splash src="www/splash.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
